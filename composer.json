{
  "name": "wmrsp/bluebillywig-youtube-importer",
  "description": "Composer package allowing to easily import video's from a YouTube channel into a Blue Billywig VMS publication.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "wmrsp",
      "email": "wimar@wimarschippers.nl"
    }
  ],
  "require": {
    "php": ">= 7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "bluebillywig/vmsrpc": "0.97",
    "norkunas/youtube-dl-php": "v1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "symfony/yaml": "^4"
  },
  "autoload": {
    "psr-4": {
      "wmrsp\\BlueBillywig\\YouTubeImporter\\": "src/"
    }
  }
}